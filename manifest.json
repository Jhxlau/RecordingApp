{
  "name": "Screen Recorder",
  "description": "Record camera, Desktop, Windows, and tabs",
  "version": "1.0.3",
  "manifest_version": 2,
  "minimum_chrome_version": "49",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "desktopCapture",
    "contextMenus",
    "audioCapture",
    "videoCapture"
  ]
  
}
